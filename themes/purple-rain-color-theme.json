  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#5b2d8e",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strings",
      "scope": ["string", "string.quoted", "string.template"],
      "settings": {
        "foreground": "#00ff87"
      }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#ffda45"
      }
    },
    {
      "name": "Constants",
      "scope": ["constant.language", "constant.character", "constant.other"],
      "settings": {
        "foreground": "#ffda45"
      }
    },
    {
      "name": "Keywords",
      "scope": ["keyword", "keyword.control", "keyword.operator.new", "keyword.operator.expression", "keyword.operator.logical", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#ff59d6"
      }
    },
    {
      "name": "Operators",
      "scope": ["keyword.operator", "punctuation.accessor"],
      "settings": {
        "foreground": "#00e5ff"
      }
    },
    {
      "name": "Functions",
      "scope": ["entity.name.function", "meta.function-call", "support.function"],
      "settings": {
        "foreground": "#7b5eff"
      }
    },
    {
      "name": "Function Parameters",
      "scope": ["variable.parameter", "meta.parameter"],
      "settings": {
        "foreground": "#ff8de6",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Classes & Types",
      "scope": ["entity.name.type", "entity.name.class", "support.type", "support.class", "entity.other.inherited-class"],
      "settings": {
        "foreground": "#ffda45",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Interfaces & Type Parameters",
      "scope": ["entity.name.type.interface", "entity.name.type.parameter"],
      "settings": {
        "foreground": "#5cefff",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "variable.other", "variable.language"],
      "settings": {
        "foreground": "#e8e0f0"
      }
    },
    {
      "name": "Properties",
      "scope": ["variable.other.property", "variable.other.object.property", "support.variable.property"],
      "settings": {
        "foreground": "#a78bfa"
      }
    },
    {
      "name": "Tags (HTML/JSX)",
      "scope": ["entity.name.tag", "punctuation.definition.tag"],
      "settings": {
        "foreground": "#ff3860"
      }
    },
    {
      "name": "Tag Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#ffda45",
        "fontStyle": "italic"
      }
    },
    {
      "name": "CSS Properties",
      "scope": ["support.type.property-name.css"],
      "settings": {
        "foreground": "#00e5ff"
      }
    },
    {
      "name": "CSS Values",
      "scope": ["support.constant.property-value.css", "support.constant.color"],
      "settings": {
        "foreground": "#ff59d6"
      }
    },
    {
      "name": "CSS Selectors",
      "scope": ["entity.other.attribute-name.class.css", "entity.other.attribute-name.id.css"],
      "settings": {
        "foreground": "#00ff87"
      }
    },
    {
      "name": "Regex",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#ff6b8a"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#00e5ff"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation", "meta.brace"],
      "settings": {
        "foreground": "#c4b5d4"
      }
    },
    {
      "name": "Decorators / Annotations",
      "scope": ["meta.decorator", "punctuation.decorator"],
      "settings": {
        "foreground": "#ffda45"
      }
    },
    {
      "name": "JSON Keys",
      "scope": ["support.type.property-name.json"],
      "settings": {
        "foreground": "#a78bfa"
      }
    },
    {
      "name": "Markdown Headings",
      "scope": ["markup.heading", "entity.name.section.markdown"],
      "settings": {
        "foreground": "#ff59d6",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#ffda45",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#00e5ff",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Links",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#7b5eff"
      }
    },
    {
      "name": "Markdown Code",
      "scope": ["markup.inline.raw", "markup.fenced_code.block"],
      "settings": {
        "foreground": "#00ff87"
      }
    },
    {
      "name": "Diff Added",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#00ff87"
      }
    },
    {
      "name": "Diff Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#ff3860"
      }
    },
    {
      "name": "Diff Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#ffda45"
      }
    }
  ]
}
